GET     /                           controllers.HomeController.index

POST    /newgame                    controllers.HomeController.newGame()

GET     /play                       controllers.HomeController.play(position: Option[java.lang.Integer])

POST    /setupGame                  controllers.HomeController.setupGame()

GET     /choosePlayers              controllers.HomeController.playerTypes()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
